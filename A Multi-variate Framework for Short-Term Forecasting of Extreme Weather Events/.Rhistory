## Normal Distribution, as a function
dens_norm <- function(x, mean = 52, v = 4){
P = (1/sqrt(2*pi*v))*exp(-(x-mean)^2/(2*v))
return (P)
}
## See result of both approches
x <- c(47, 50, 52, 54, 55)
dnorm(x, mean = 52, sd = sqrt(4))
dens_norm(x)
# Function plot() can be used to plot functions:
# Argument: type="l" will draw lines and not points
data.points   <- data.frame(x=seq(47, 57, by=0.2))             #x-axis set of points
data.points$y <- dens_norm(data.points$x)                          #y-axis set of points
plot(y ~ x, data=data.points, type="l", ylab="Density", main="Normal distributionnnwith mean 52 and var 4")
## Area under the normal dis can be obtained through two approches
##integrate function
integrate(f = dens_norm, lower = -Inf, upper = 54)
